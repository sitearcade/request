{
  "name": "@sitearcade/request",
  "version": "0.5.1",
  "description": "Fetch-wrapper with some useful features.",
  "license": "MIT",
  "homepage": "https://www.sitearcade.com",
  "repository": "github:sitearcade/request",
  "bugs": "https://github.com/sitearcade/request/issues",
  "author": {
    "name": "Mike Stop Continues",
    "email": "mike@sitearcade.com",
    "url": "https://www.sitearcade.com"
  },
  "sideEffects": false,
  "source": "src/index.ts",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.js",
  "jsnext:main": "./dist/index.modern.js",
  "module": "./dist/index.module.js",
  "syntax": {
    "esmodules": "./dist/index.modern.js"
  },
  "exports": {
    ".": {
      "browser": "./dist/index.module.js",
      "import": "./dist/index.module.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "microbundle watch --no-compress -f cjs,esm,modern",
    "build": "microbundle --no-compress -f cjs,esm,modern",
    "clean": "rm -rf dist",
    "test": "NODE_ENV=testing jest --passWithNoTests --coverage",
    "test:ci": "NODE_ENV=testing jest --passWithNoTests --ci",
    "test:watch": "NODE_ENV=testing jest --passWithNoTests --coverage --watch",
    "preversion": "npm run build && npm test",
    "postversion": "npm publish --access public",
    "prepublishOnly": "npm run clean && npm run build",
    "postpublish": "git push origin --all; git push origin --tags"
  },
  "dependencies": {
    "abort-controller": "^3.0.0",
    "isomorphic-unfetch": "^3.1.0",
    "promise-retry": "^2.0.1",
    "querystring": "^0.2.1",
    "ramda": "^0.27.1"
  },
  "devDependencies": {
    "@sitearcade/babel-preset": "1.0.2",
    "@sitearcade/browserslist-config": "0.3.1",
    "@sitearcade/eslint-config": "1.1.15",
    "@sitearcade/jest-preset": "2.2.0",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.31",
    "@types/promise-retry": "1.1.3",
    "@types/ramda": "0.27.38",
    "jest-fetch-mock": "3.0.3",
    "microbundle": "0.13.0",
    "prettier": "2.2.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "typescript": "4.2.2"
  },
  "engines": {
    "node": ">=12"
  },
  "babel": {
    "targets": {
      "node": 12
    },
    "presets": [
      "module:@sitearcade/babel-preset"
    ]
  },
  "browserslist": [
    "extends @sitearcade/browserslist-config"
  ],
  "eslintConfig": {
    "extends": [
      "@sitearcade/eslint-config"
    ]
  },
  "jest": {
    "preset": "@sitearcade/jest-preset"
  }
}
